<launch>

    <node name="gulliview_server" pkg="gulliview_server" type="gulliview" output="screen" />

    <node name="database" pkg="database" type="database_node" output="screen" />

    <node name="controller" pkg="pid" type="controller" output="screen" >
      <param name="Kp" value="5.55" />
      <param name="Ki" value="0.0" />
      <param name="Kd" value="0.0" />
    </node>

    <node name="pololu_mc" pkg="pololu_mc" type="pololu_mc" output="screen" launch-prefix="sudo" />

</launch>
